{
  "name": "react-component-template",
  "version": "0.1.0",
  "description": "a template for react component develop",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --host 0.0.0.0",
    "build": "webpack --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keep-yukap/react-component-template.git"
  },
  "keywords": [
    "react",
    "template"
  ],
  "author": "yukap",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keep-yukap/react-component-template/issues"
  },
  "homepage": "https://github.com/keep-yukap/react-component-template#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "babel-jest": "^23.6.0",
    "enzyme": "^3.6.0",
    "es5-polyfill": "0.0.6",
    "react-test-renderer": "^16.5.1",
    "regenerator-runtime": "^0.12.1"
  }
}
